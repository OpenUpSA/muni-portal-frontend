{
  "name": "muni-portal",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@lhci/cli": "^0.5.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "copyfiles": "^2.4.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "http-server": "^0.12.3",
    "import-webflow-export": "^0.0.4",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-sw-cache": "^0.3.1",
    "prettier": "^2.1.2",
    "workbox-cli": "^5.1.4"
  },
  "scripts": {
    "start": "yarn build && yarn serve",
    "build": "parcel build ./src/index.html --public-url /",
    "dev": "parcel serve -p 8080 ./src/index.html --public-url /",
    "serve": "http-server -d false -c-1 --proxy http://localhost:8080? dist",
    "serve-https": "http-server -S -C server-cert.pem -K server-cert-key.pem -d false -c-1 --proxy http://localhost:8080? dist",
    "mkcert-loopback": "mkcert -cert-file server-cert.pem -key-file server-cert-key.pem 127.0.0.1"
  },
  "browserslist": "> 0.25%, not dead",
  "dependencies": {
    "@sentry/browser": "^5.27.1",
    "@sentry/tracing": "^5.27.1",
    "workbox-build": "^5.1.4",
    "workbox-routing": "^5.1.4",
    "workbox-window": "5.1.2"
  },
  "cache": {
    "strategy": "inject",
    "swSrc": "src/service-worker.js",
    "swDest": "service-worker.js",
    "globDirectory": "dist",
    "globPatterns": [
      "**/*.{html,js,css,jpg,png,gif,svg,eot,ttf,woff,woff2,webmanifest}"
    ]
  },
  "importWebflowExport": {
    "importHtml": [
      {
        "glob": "*.html",
        "destDir": "src",
        "transforms": "./src/js/webflow/import.js"
      }
    ]
  }
}
